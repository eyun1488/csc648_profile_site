<div class="profile">
    {{#each person}}
    <a href="/about/{{this.first_name}}" style="text-decoration:none">
        <img src="/images/{{this.image}}" alt="imageNoWork" style="height:100px">
        <p>Name: {{this.first_name}} {{this.last_name}}</p>
        <p>GitHub Name: {{this.github}}</p>
        <p>About Me: {{this.information}}</p>
    </a>
    {{/each}}
</div>